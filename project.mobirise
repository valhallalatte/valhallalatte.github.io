          "_name": "formbuilder",
          "_customHTML": "<section class=\"form\" group=\"Form\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Style</header>\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\">\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\">\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#efefef\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#cccccc\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#c1c1c1\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../assets/background.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div mbr-form=\"\" class=\"col-lg-8 mx-auto mbr-form\">\n<!--Formbuilder Form-->\n<form action=\"Clinic100115@fmc-na.com\" name=\"Book\" method=\"POST\" class=\"mbr-form form-with-styler\">\n<div class=\"form-row\">\n<div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thank you for submitting your request. We will contact you shortly for further information.</div>\n<div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">Oops...! some problem!</div>\n</div>\n<div class=\"dragArea form-row\">\n<div class=\"col-lg-12\">\n<h4 mbr-theme-style=\"display-5\" class=\"mbr-fonts-style\">Make a Reservation</h4>\n</div>\n<div class=\"col-lg-12\">\n<hr>\n</div>\n<div class=\"col-lg-12\">\n<p mbr-theme-style=\"display-7\" class=\"mbr-fonts-style\">Customer Information</p>\n</div>\n<div class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-control-label mbr-fonts-style\">Name</label>\n<div class=\"form-row\">\n<div class=\"col\">\n<input type=\"text\" name=\"nameFirst\" placeholder=\"First\" data-form-field=\"nameFirst\" class=\"form-control text-multiple\" value=\"\">\n</div>\n<div class=\"col\">\n<input type=\"text\" name=\"nameLast\" placeholder=\"Last\" data-form-field=\"nameLast\" class=\"form-control text-multiple\" value=\"\">\n</div>\n</div>\n</div>\n<div data-for=\"gender\" class=\"col form-group\">\n<div class=\"form-control-label\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"gender\" class=\"mbr-fonts-style\">Gender</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"gender\" data-form-field=\"gender\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Male\" checked=\"checked\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Male</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"gender\" data-form-field=\"gender\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Female\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Female</label>\n</div>\n</div>\n<div class=\"col-auto\">\n<p mbr-theme-style=\"display-7\" class=\"mbr-fonts-style\">DOB</p>\n</div>\n<div class=\"col-lg-3 form-group\" data-for=\"birthyear\" style=\"\">\n<input type=\"number\" name=\"birthyear\" placeholder=\"YEAR\" max=\"2100\" min=\"1900\" step=\"1\" data-form-field=\"birthyear\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"birthmonth\" class=\"col-lg-3 form-group\">\n<input type=\"number\" name=\"birthmonth\" placeholder=\"MONTH\" max=\"12\" min=\"1\" step=\"1\" data-form-field=\"birthmonth\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"birthday\" class=\"col-lg-3 form-group\">\n<input type=\"number\" name=\"birthday\" placeholder=\"DAY\" max=\"31\" min=\"1\" step=\"1\" data-form-field=\"birthday\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"phone\" class=\"col-lg-12 col-sm-12 form-group\">\n<input type=\"text\" name=\"phone\" placeholder=\"phone number\" data-form-field=\"phone\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"email\" class=\"col-lg-12 col-sm-12 form-group\">\n<input type=\"email\" name=\"email\" placeholder=\"E-Mail\" data-form-field=\"email\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"address\" class=\"col-lg-12 col-sm-12 form-group\">\n<textarea name=\"address\" placeholder=\"Address\" data-form-field=\"address\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\"></textarea>\n</div>\n<div class=\"col-lg-12\">\n<p mbr-theme-style=\"display-7\" class=\"mbr-fonts-style\">Dialysis Information</p>\n</div>\n<div class=\"col-auto\">\n<p mbr-theme-style=\"display-7\" class=\"mbr-fonts-style\">Tx Time</p>\n</div>\n<div data-for=\"durationtxhour\" class=\"col-auto form-group\">\n<input type=\"number\" name=\"durationtxhour\" placeholder=\"hour\" max=\"12\" min=\"1\" step=\"1\" data-form-field=\"durationtxhour\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"durationtxminute\" class=\"col-auto form-group\">\n<input type=\"number\" name=\"durationtxminute\" placeholder=\"min\" max=\"59\" min=\"0\" step=\"1\" data-form-field=\"durationtxminute\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"accesstype\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"accesstype\" class=\"form-control-label mbr-fonts-style\">Access Information</label>\n<input type=\"text\" name=\"accesstype\" placeholder=\"access type (fistula, graft, catheter)\" data-form-field=\"accesstype\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"accesslocation\" class=\"col-lg-12 col-sm-12 form-group\">\n<input type=\"text\" name=\"accesslocation\" placeholder=\"access location (left forearm, right upper arm)\" data-form-field=\"accesslocation\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-control-label mbr-fonts-style\">Nephrologist's Name</label>\n<div class=\"form-row\">\n<div class=\"col\">\n<input type=\"text\" name=\"nephrologistFirst\" placeholder=\"First\" data-form-field=\"nephrologistFirst\" class=\"form-control text-multipl